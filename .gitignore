# Ignore node_modules directory (if using Node.js)
node_modules/

# Ignore build directories and files
dist/
build/

# Ignore environment variables file (if using environment variables)
.env

# Ignore log files
*.log

# Ignore temporary files
*.tmp
*.temp

# Ignore system-specific files
.DS_Store       # macOS
Thumbs.db       # Windows
*.swp           # Vim swap files
*.swo
*.bak           # Backup files
*.orig          # Merge resolution files

# Ignore minified files (if you don't want to track them)
*.min.js
*.min.css

# Ignore Sass/CSS preprocessor cache files
.sass-cache/
*.css.map
*.scssc

# Ignore package-lock.json or yarn.lock (if you don't want to track it)
# Uncomment the one you use
# package-lock.json
# yarn.lock

# Ignore npm debug file
npm-debug.log*

# Ignore coverage reports
coverage/

# Ignore generated documentation
docs/

# Ignore compiled or transpiled files (if using a transpiler like Babel)
lib/
